{% form_theme replyForm.form '::baseForm.html.twig' %}

{% block formReply %}
    <form id="replyForm" action="{{ replyForm.action }}" method="post" autocomplete="off" {{ form_enctype(replyForm.form ) }} >
        <div class="form_row">
            <div class="clearfix"></div>
        </div>
        {{ form_widget(replyForm.form ) }}
        <div class="button">
            <input type="submit" class="button" value="Plaatsen" />
            <div class="clearfix"></div>
        </div>
    </form>
{% endblock %}